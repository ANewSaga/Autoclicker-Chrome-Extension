{
	"name": "Auto Clicker (working)",
	"description": "oops",
	"author": "saga",
	"manifest_version": 3,
	"version": "1.0",
	"action": {
		"default_title": "Auto Clicker (working)",
		"default_popup": "index.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"run_at": "document_end",
			"js": ["Main.js"],
			"all_frames": true
		}
	],
	"icons": {
		"16": "imgs/icon16.png",
		"48": "imgs/icon48.png",
		"128": "imgs/icon128.png"
	},
	"permissions": ["storage"]
}
